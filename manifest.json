    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "workout_data",
          "accept": ["application/json", ".json"]
        }
      ]
    }
  },
